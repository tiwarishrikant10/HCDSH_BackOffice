<div class="view-port">
    <div class="view bg-white">
        <div class="card card-default">
            <div class="card-block sub-box table-responsive filtr-srch p-t-0">
               
                @{int i = 1;}
                <table id="example" class="table table-hover demo-table-dynamic table-responsive-block m-t-0" cellspacing="0" width="100%">
                    <thead>
                        <tr>
                            <th>Sno.</th>
                            <th>User Name</th>
                            <th>Email</th>
                            <th>Mobile</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@i</td>
                                <td>@item.user_name</td>
                                <td>@item.email_id</td>
                                <td>@item.mobile</td>
                            </tr>
                            i++;
                        }
                    </tbody>
                </table>
            </div>

        </div>
